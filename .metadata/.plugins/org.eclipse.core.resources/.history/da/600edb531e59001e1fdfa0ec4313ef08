package domain;

public class pagingVO 
{
	private int pageNo;
	private int qty;
	
	public pagingVO()
	{
		this.pageNo=1;
		this.qty=10;
	}
	
	public pagingVO(int pageNo,int qty)
	{
		this.pageNo=pageNo;
		this.qty=qty;
	}
	//---------------------------------getter/setter------------------------------

	public int getPageNo() {
		return pageNo;
	}

	public void setPageNo(int pageNo) {
		this.pageNo = pageNo;
	}

	public int getQty() {
		return qty;
	}

	public void setQty(int qty) {
		this.qty = qty;
	}
	

}
